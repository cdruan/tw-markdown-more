{
  "name": "tw-markdown-more",
  "version": "1.0.0",
  "description": "TiddlyWiki plugin for externalizing TW core and plugins",
  "dependencies": {
    "tiddlywiki": ">=5.2.6"
  },
  "scripts": {
    "build": "TIDDLYWIKI_PLUGIN_PATH=plugins node ./node_modules/tiddlywiki/tiddlywiki.js wiki --build index",
    "start": "TIDDLYWIKI_PLUGIN_PATH=plugins node ./node_modules/tiddlywiki/tiddlywiki.js wiki --build listen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdruan/tw-markdown-more.git"
  },
  "keywords": [
    "TW5",
    "TW5-plugins",
    "tiddlywiki"
  ],
  "author": "cdruan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdruan/tw-markdown-more/issues"
  },
  "homepage": "https://github.com/cdruan/tw-markdown-more#readme"
}
